   "source": [
    "def solution(k, d):\n",
    "    r = d/k\n",
    "    answer = int(r)+1\n",
    "    for y in range(0, int(r)+1):\n",
    "        answer += int((r**2-y**2)**0.5)\n",
    "    return answer"
   ]
