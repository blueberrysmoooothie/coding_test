    "def solution(n, s):\n",
    "    if s<n:\n",
    "        return [-1]\n",
    "    answer = [s//n for i in range(n)]\n",
    "    for i in range(s%n):\n",
    "        answer[n-i-1]+=1\n",
    "    return answer"
   ]
  },
